<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_part_1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_part_1">
        <activity android:name=".ActivityOther" android:parentActivityName=".MainActivity"
            android:label="Activity Other"/>
        <activity android:name=".ActivityProfiles" android:parentActivityName=".MainActivity"
            android:label="Activity Profiles"/>
        <activity android:name=".ActivitySettings" android:parentActivityName=".MainActivity"
            android:label="Activity Settings"/>
        <activity android:name=".ActivityFavorite" android:parentActivityName=".MainActivity"
            android:label="Activity Favorite"/>
        <activity android:name=".ActivityInfo" android:parentActivityName=".MainActivity"
            android:label="Activity Info"/>
        <activity android:name=".MainActivity" android:label="Main Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>